<% if notice %>
<p id="notice"><%= noitce %></p>
<% end %>

<h1> MY STORE CATALOG</h1>

<% @products.each do |product| %>
<div class="entry">
<%= image_tag(product.image_url) %>
<h3><%=product.title %></h3>
<h2><%= sanitize(product.description) %></h2>
<div class="price_line">
<span class="price"><%= number_to_currency(product.price) %></span>
</div>
</div>
<% end %>
